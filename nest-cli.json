{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": {
      "type": "swc",
      "options": {
        "swcrcPath": "./infra/swc/.swcrc",
        "typeCheck": true,
        "outDir": "./dist"
      }
    },
    "deleteOutDir": true
  }
}
